<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br><br>
  <%= f.text_field "notes_content[]" %>
  <!--<input type="textbox" name="song_notes_attributes_1_content">
  <input type="textbox" name="song_notes_attributes[]">
  <input type="textbox" name="song_notes_attributes[]">
  <input type="textbox" name="song_notes_attributes[]">-->
  <br>

  <%= f.submit %>
<% end %>


